IdentityFile ~/.ssh/id_rsa

Host *
    UseKeychain yes
    AddKeysToAgent yes
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    XAuthLocation /opt/X11/bin/xauth
    HostkeyAlgorithms +ssh-rsa

# github.com
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa
